        <mxCell id="MbbgzbBlrMw2aK5pjZrs-1" value="&lt;h3 style=&quot;caret-color: rgb(0, 0, 0);&quot; class=&quot;&quot; data-end=&quot;220&quot; data-start=&quot;163&quot;&gt;&lt;strong data-end=&quot;220&quot; data-start=&quot;167&quot;&gt;MÓDULO DE EXPERIENCIA DE USUARIOS:&amp;nbsp;&lt;/strong&gt;&lt;/h3&gt;" style="rounded=0;whiteSpace=wrap;html=1;fillColor=#d5e8d4;strokeColor=#82b366;align=center;fontSize=10;fontFamily=Verdana;" vertex="1" parent="1">
