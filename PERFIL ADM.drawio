        <mxCell id="Rri0frco-eFaM65np2tf-4" value="&amp;nbsp;DESDE EL PERFIL DAMINISTRATIVO&amp;nbsp;" style="rounded=0;whiteSpace=wrap;html=1;fillColor=#d5e8d4;strokeColor=#82b366;fontSize=10;align=center;fontFamily=Verdana;" vertex="1" parent="1">
