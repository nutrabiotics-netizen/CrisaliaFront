        <mxCell id="LwXHryfnUARGS76Rn4A5-22" value="&lt;h3 style=&quot;caret-color: rgb(0, 0, 0);&quot; class=&quot;&quot; data-end=&quot;220&quot; data-start=&quot;163&quot;&gt;&lt;strong data-end=&quot;220&quot; data-start=&quot;167&quot;&gt;GESTIÓN DE DEMORAS&lt;/strong&gt;&lt;/h3&gt;" style="rounded=0;whiteSpace=wrap;html=1;fillColor=#FF9933;strokeColor=#82b366;align=center;fontSize=10;fontFamily=Verdana;" vertex="1" parent="1">
          <mxGeometry x="-4890.22" y="-4770" width="220" height="60" as="geometry" />
        </mxCell>
        <mxCell id="LwXHryfnUARGS76Rn4A5-26" style="edgeStyle=orthogonalEdgeStyle;rounded=0;orthogonalLoop=1;jettySize=auto;html=1;entryX=0.511;entryY=-0.018;entryDx=0;entryDy=0;entryPerimeter=0;" edge="1" parent="1" source="NVWyPv4ccImoruTVmd1k-1" target="NVWyPv4ccImoruTVmd1k-2">
