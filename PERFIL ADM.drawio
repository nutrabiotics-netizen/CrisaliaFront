        <mxCell id="SQNqiSdsgoqq1kZhoihM-36" value="&lt;h3&gt;&lt;b style=&quot;background-color: transparent; color: light-dark(rgb(0, 0, 0), rgb(255, 255, 255)); font-size: 12px;&quot;&gt;FASE 5: Almacenamiento y Auditoría&lt;/b&gt;&lt;/h3&gt;&lt;p&gt;&lt;/p&gt;" style="rounded=0;whiteSpace=wrap;html=1;fillColor=#d5e8d4;strokeColor=#82b366;" vertex="1" parent="1">
